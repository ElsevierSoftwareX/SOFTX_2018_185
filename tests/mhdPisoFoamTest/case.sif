Header
  CHECK KEYWORDS Warn
  Mesh DB "." "mesh_Elmer"
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 12
  Coordinate System = Cartesian
  Coordinate Mapping(3) = 1 2 3
  Angular Frequency = 628.32

  Simulation Type = Transient
  Timestepping Method = BDF
  BDF Order = 2
  Timestep Sizes = 0.005
  Timestep Intervals = 11
  Steady State Max Iterations = 1


  Solver Input File = case.sif
  Post File = case.vtu
  Output Intervals(1) = 1
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
  Permittivity of Vacuum = 8.8542e-12
  Permeability of Vacuum = 1.257e-6
  Boltzmann Constant = 1.3807e-23
  Unit Charge = 1.602e-19
End

Initial Condition 1
  Temperature = 0
  Velocity 1 = 0
  Velocity 2 = 0
  Velocity 3 = 0
End

Body 1
  Target Bodies(1) = 6
  Name = "Wire"
  Equation = 1
  Material = 1
  Body Force = 1
  Initial Condition = 1
End

Body 2
  Target Bodies(1) = 7
  Name = "Cylinder"
  Equation = 1
  Material = 2
  Body Force = 2
  Initial Condition = 1
End

Body 3
  Target Bodies(1) = 8
  Name = "Air"
  Equation = 1
  Material = 1
  Initial Condition = 1
End

Solver 1
  Equation = OpenFOAM2Elmer
  Procedure = "OpenFOAM2Elmer" "OpenFOAM2ElmerSolver"

  Target Variable 1 = String "Electric Conductivity"
End 

Solver 2
  Equation = "MGDynamics"

  Variable = "AV[AV re:1 AV im:1]"
  Procedure = "MagnetoDynamics" "WhitneyAVHarmonicSolver"

  Linear System Symmetric = True
  Linear System Solver = "Iterative"
  Linear System Preconditioning = none
  Linear System Residual Output = 50
  Linear System Max Iterations = 1000
  Linear System Iterative Method = BiCGStabl

  Linear System Convergence Tolerance = 1.0e-8
  BicgStabl Polynomial Degree = 4

  Exported Variable 1 = String "OF conductivity"
End

Solver 3
  Equation = "MGDynamicsCalc"

  Procedure = "MagnetoDynamics" "MagnetoDynamicsCalcFields"
  Linear System Symmetric = True

  Potential Variable = String "AV"

  Calculate Current Density = Logical True
  Calculate Electric Field = Logical True
  Calculate Magnetic Field Strength = Logical True
  Calculate JxB = Logical True
  Calculate Joule Heating = True

  Steady State Convergence Tolerance = 0
  Linear System Solver = "Iterative"
  Linear System Preconditioning = None
  Linear System Residual Output = 0
  Linear System Max Iterations = 5000
  Linear System Iterative Method = CG
  Linear System Convergence Tolerance = 1.0e-8

  Impose Body Force Potential = Logical True
  Impose Body Force Current = Logical True

  Discontinuous Bodies = True
  Calculate harmonic peak power = logical true
End

Solver 4
  Equation = ElmerToOpenFOAM
  Procedure = "Elmer2OpenFOAM" "Elmer2OpenFOAMSolver"

  Target Variable 1 = joule heating
  Target Variable 2 = jxb re 1
  Target Variable 3 = jxb re 2
  Target Variable 4 = jxb re 3
End 

Equation 1
  Name = "MGDyn"
  Active Solvers(4) = 1 2 3 4
  Magnetic Induction = Logical True
End

Material 1
  Name = "Air"
  Electric Conductivity = 0
  Relative Permittivity = 1
  Relative Permeability = 1
End

Material 2
  Name = "Conductor"
  Electric Conductivity = Equals "OF conductivity"
  Relative Permittivity = 1
  Relative Permeability = 1
End

Body Force 1
  Name = "Current"

  Current Density im 1 = Variable coordinate
    Real MATC "-1e8*tx(1)"

  Current Density im 2 = Variable coordinate
    Real MATC "1e8*tx(0)"
End

Body Force 2
  Lorentz Force = Logical True
  Joule Heat = Logical True
End

Boundary Condition 1
  Target Boundaries(1) = 4
  Name = "farfield"
  P re {e} = real 0
  P im {e} = real 0
End
