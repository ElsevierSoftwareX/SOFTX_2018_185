// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (melt air);

melt
{
    transportModel  Newtonian;
    nu              [0 2 -1 0 0 0 0] 1e-4;
    rho             [1 -3 0 0 0 0 0] 2380.0;
}

air
{
    transportModel  Newtonian;
    nu              [0 2 -1 0 0 0 0] 1e-4; //-5
    rho             [1 -3 0 0 0 0 0] 5;
}

sigma               [1 0 -2 0 0 0 0] 0.94;
elcond_ref          [0 0 0 0 0 0 0] 3850000.0;

